{
  "seed": "random",
  "num_envs": 32,
  "max_env_steps": 256,
  "update_steps": 60000,
  "updates_per_jit": 100,
  "environment": {
    "env_type": "return",
    "num_agents": 16
  },
  "learner": {
    "optimizer": {
      "type": "adamw",
      "learning_rate": 0.001,
      "weight_decay": 0.0,
      "eps": 1e-8,
      "beta1": 0.9,
      "beta2": 0.999,
      "max_norm": 0.1
    },
    "model": {
      "obs_encoder": {
        "obs_type": "grid_cnn"
      },
      "hidden_features": 128,
      "transformer_block": {
        "num_heads": 4,
        "ffn_size": 512,
        "rope_max_wavelength": 10000.0,
        "glu": false,
        "gtrxl_gate": false,
        "gtrxl_bias": 1.0,
        "attention_softcap": null
      },
      "num_layers": 3,
      "activation": "gelu",
      "norm": "layer_norm",
      "kernel_init": "glorot_uniform",
      "dtype": "bfloat16",
      "param_dtype": "float32"
    },
    "trainer": {
      "trainer_type": "ppo",
      "minibatch_count": 1,
      "vf_coef": 0.5,
      "entropy_coef": 0.001,
      "vf_clip": 0.11657459349860262,
      "discount": 0.9271513652161643,
      "gae_lambda": 0.9
    }
  },
  "logger": {
    "log_rate": 1000,
    "use_console": true,
    "use_tb": false,
    "use_neptune": false,
    "use_wandb": true
  }
}
