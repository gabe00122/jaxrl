{
  "seed": "random",
  "num_envs": 32,
  "max_env_steps": 256,
  "update_steps": 20000,
  "updates_per_jit": 10,
  "environment": {
    "env_type": "return",
    "num_agents": 16
  },
  "learner": {
    "optimizer": {
      "type": "adamw",
      "learning_rate": 0.0009442327101842291,
      "weight_decay": 0.0001215732069346454,
      "eps": 1e-8,
      "beta1": 0.9,
      "beta2": 0.999,
      "max_norm": 0.2483780769403615
    },
    "model": {
      "obs_encoder": {
        "obs_type": "grid_cnn"
      },
      "hidden_features": 128,
      "transformer_block": {
        "attention_impl": "xla",
        "num_heads": 4,
        "num_kv_heads": 4,
        "head_dim": 32,
        "ffn_size": 512,
        "rope_max_wavelength": 10000.0,
        "glu": false,
        "gtrxl_gate": false,
        "gtrxl_bias": 1.0
      },
      "num_layers": 3,
      "activation": "gelu",
      "norm": "layer_norm",
      "kernel_init": "glorot_uniform",
      "dtype": "bfloat16",
      "param_dtype": "float32"
    },
    "trainer": {
      "trainer_type": "ppo",
      "normalize_advantage": true,
      "minibatch_count": 1,
      "vf_coef": 1.401531000697826,
      "entropy_coef": 0.0007561531481145621,
      "vf_clip": 0.2717602880463028,
      "discount": 0.9623508086831543,
      "gae_lambda": 0.9700622054336805
    }
  },
  "logger": {
    "log_rate": 1000,
    "use_console": true,
    "use_tb": false,
    "use_neptune": false,
    "use_wandb": true
  }
}
