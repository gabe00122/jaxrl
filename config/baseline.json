{
  "seed": "random",
  "num_envs": 128,
  "max_env_steps": 512,
  "update_steps": 20000,
  "updates_per_jit": 10,
  "environment": {
    "env_type": "return_digging",
    "num_agents": 16
  },
  "hl_gauss": {
    "min_value": 0.0,
    "max_value": 10.0,
    "n_logits": 51,
    "sigma": 0.75
  },
  "learner": {
    "optimizer": {
      "type": "adamw",
      "learning_rate": 0.0009442327101842291,
      "weight_decay": 0.0001215732069346454,
      "eps": 1e-8,
      "beta1": 0.9,
      "beta2": 0.999,
      "max_norm": 0.2483780769403615
    },
    "model": {
      "obs_encoder": {
        "obs_type": "grid_cnn"
      },
      "hidden_features": 128,
      "transformer_block": {
        "attention_impl": "cudnn",
        "num_heads": 4,
        "num_kv_heads": 1,
        "head_dim": 32,
        "ffn_size": 768,
        "rope_max_wavelength": 10000.0,
        "glu": true
      },
      "num_layers": 6,
      "activation": "gelu",
      "norm": "rms_norm",
      "kernel_init": "glorot_uniform",
      "dtype": "bfloat16",
      "param_dtype": "float32"
    },
    "trainer": {
      "trainer_type": "ppo",
      "minibatch_count": 8,
      "epoch_count": 1,
      "vf_coef": 1.401531000697826,
      "entropy_coef": 0.00005,
      "vf_clip": 0.2717602880463028,
      "discount": 0.9623508086831543,
      "gae_lambda": 1.0
    }
  },
  "logger": {
    "log_rate": 1000,
    "use_console": true,
    "use_tb": false,
    "use_neptune": false,
    "use_wandb": true
  }
}
