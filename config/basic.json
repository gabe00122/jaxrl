{
    "logger": {
        "use_neptune": true
    },
    "learner": {
        "optimizer": {
            "type": "adamw",
            "learning_rate": 0.0012,
            "warmup_steps": 200,
            "beta1": 0.9,
            "beta2": 0.95,
            "eps": 1e-12,
            "weight_decay": 0.1
        },
        "model": {
            "activation": "relu",
            "body": {
                "type": "cnn",
                "layers": [
                    {
                        "features": 32,
                        "kernel_size": [2, 8, 8],
                        "stride": [1, 4, 4]
                    },
                    {
                        "features": 64,
                        "kernel_size": [2, 4, 4],
                        "stride": [1, 2, 2]
                    },
                    {
                        "features": 64,
                        "kernel_size": [2, 3, 3],
                        "stride": [1, 1, 1]
                    }
                ],
                "output_size": 512
            }
        }
    },
    "environment": {
        "backend": "envpool",
        "name": "Pong-v5",
        "num_envs": 10,
        "max_steps": 1000
    }
}