{
    "logger": {
        "use_wandb": true,
        "use_console": true,
        "use_tb": true
    },
    "learner": {
        "entropy_coefficient": 0.001,
        "actor_coefficient": 0.5,
        "optimizer": {
            "type": "adamw",
            "learning_rate": 0.0001,
            "beta1": 0.9,
            "beta2": 0.999,
            "eps": 1e-8,
            "weight_decay": 0.001
        },
        "model": {
            "activation": "relu",
            "body": {
                "type": "cnn",
                "layers": [
                    {
                        "features": 32,
                        "kernel_size": [2, 8, 8],
                        "stride": [1, 4, 4]
                    },
                    {
                        "features": 64,
                        "kernel_size": [2, 4, 4],
                        "stride": [1, 2, 2]
                    },
                    {
                        "features": 64,
                        "kernel_size": [2, 3, 3],
                        "stride": [1, 1, 1]
                    }
                ],
                "output_size": 512
            }
        }
    },
    "environment": {
        "backend": "envpool",
        "name": "DemonAttack-v5",
        "num_envs": 64,
        "max_steps": 7000000
    }
}