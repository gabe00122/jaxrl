{
  "seed": 75264,
  "num_envs": 128,
  "max_env_steps": 1024,
  "updates_per_jit": 1,
  "update_steps": 15000,
  "learner": {
    "optimizer": {
      "type": "adamw",
      "learning_rate": 0.0002,
      "weight_decay": 0.0,
      "eps": 1e-8,
      "beta1": 0.9,
      "beta2": 0.999,
      "max_norm": 1.0
    },
    "model": {
      "obs_encoder": {
        "obs_type": "linear"
      },
      "hidden_features": 256,
      "layer": {
        "feed_forward": {
          "glu": true,
          "size": 768
        },
        "history": {
          "type": "attention",
          "num_heads": 8,
          "num_kv_heads": 4,
          "head_dim": 32,
          "sliding_window": null,
          "rope_max_wavelength": 10000.0,
          "use_qk_norm": false
        },
        "use_post_attn_norm": false,
        "use_post_ffw_norm": false
      },
      "num_layers": 2,
      "value_hidden_dim": 768,
      "value": {
        "type": "mse"
      },
      "activation": "gelu",
      "norm": "rms_norm",
      "kernel_init": "glorot_uniform",
      "dtype": "bfloat16",
      "param_dtype": "float32"
    },
    "trainer": {
      "trainer_type": "ppo",
      "epoch_count": 4,
      "minibatch_count": 8,
      "vf_coef": 0.5,
      "entropy_coef": 0.002,
      "vf_clip": 0.2,
      "discount": 0.999,
      "gae_lambda": 0.8,
      "normalize_advantage": true
    }
  },
  "environment": {
    "env_type": "craftax"
  },
  "logger": {
    "use_console": true,
    "use_tb": false,
    "use_neptune": false,
    "use_wandb": true
  }
}
